{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tsc:watch": "tsc --noEmit --watch",
    "tsc:dev": "npm run dev & npm run tsc:watch",
    "build": "tsc --noEmit && vite build",
    "start": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "fm:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix",
    "clean": "rm -rf node_modules dist build",
    "re:dev": "npm run clean && npm install && npm run dev",
    "re:build": "npm run clean && npm install && npm run build"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/dm-sans": "^5.2.5",
    "@fontsource/barlow": "^5.2.5",
    "@iconify/react": "^6.0.0",
    "@mui/lab": "^7.0.0-beta.12",
    "@mui/material": "^7.1.0",
    "@tanstack/react-query": "^5.76.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "apexcharts": "^4.7.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.37.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^5.1.0",
    "minimal-shared": "^1.0.11",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "simplebar-react": "^3.3.1",
    "vite-plugin-checker": "^0.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.4.1",
    "cypress": "^14.3.3",
    "cypress-slow-down": "^1.3.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
